<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Ms</title>
    <link rel="stylesheet" href="CSS/global.css" />
    <link rel="stylesheet" href="/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css">
    <link rel="shortcut icon" href="/images/favicon.png" type="image/x-icon" />
  </head>

  <style>
    .sidebar {
      position: fixed;
      top: 0;
      bottom: 0;
      left: 0;
      z-index: 100;
      padding: 48px 0 0;
      box-shadow: inset -1px 0 0 rgba(0, 0, 0, 0.1);
    }

    .nav-link {
      color: #adb5bd;
      margin-bottom: 5px;
      border-radius: 5px;
    }

    .nav-link:hover,
    .nav-link.active {
      color: #fff;
      background-color: rgba(255, 255, 255, 0.1);
    }

    .nav-link i {
      margin-right: 4px;
      color: #6c757d;
    }

    .nav-link:hover i,
    .nav-link.active i {
      color: #fff;
    }

    .progress-circle {
      width: 100px;
      height: 100px;
      margin: 0 auto;
      position: relative;
      background-color: #f8f9fa;
      border-radius: 50%;
    }

    .progress-circle-value {
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
      font-size: 1.5rem;
      font-weight: bold;
      color: #0d6efd;
    }

    main {
      margin-left: 280px; /* Adjust based on sidebar width */
      padding-top: 1rem;
    }

    /* Offcanvas styling for mobile */
    .offcanvas-start {
      width: 280px;
      background-color: #212529;
    }

    /* Mobile header with menu button */
    .mobile-header {
      display: none;
      padding: 1rem;
      background-color: #f8f9fa;
      border-bottom: 1px solid #dee2e6;
    }

    @media (max-width: 991.98px) {
      main {
        margin-left: 0;
      }
      
      .mobile-header {
        display: flex;
        justify-content: space-between;
        align-items: center;
      }
      
      .sidebar {
        display: none;
      }
    }

    @media (min-width: 992px) {
      .offcanvas-backdrop {
        display: none !important;
      }
    }
  </style>
  <body>
<div class="container-fluid">
  <div class="row">
    <!-- Mobile Header (visible only on small screens) -->
    <div class="mobile-header">
      <h5 class="mb-0">Dashboard</h5>
      <button class="btn btn-primary" type="button" data-bs-toggle="offcanvas" data-bs-target="#sidebar" aria-controls="sidebar">
        <i class="bi bi-list"></i> Menu
      </button>
    </div>

    <!-- Sidebar Navigation - Offcanvas for mobile -->
    <div class="offcanvas offcanvas-start" tabindex="-1" id="sidebar" aria-labelledby="sidebarLabel">
      <div class="offcanvas-header">
        <h5 class="offcanvas-title" id="sidebarLabel">Menu</h5>
        <button type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
      </div>
      <div class="offcanvas-body">
        <div class="position-sticky pt-3">
          <div class="text-center mb-4">
            <% if(role === 'student') { %>
               <img src="/images/student_avatar.png" class="rounded-circle" width="80" alt="User Avatar">
              <%}%>
           
              <% if(role === 'parent') { %>
               <img src="/images/parent_avatar.png" class="rounded-circle" width="80" alt="User Avatar">
              <%}%>

              <% if(role === 'teacher') { %>
               <img src="/images/teacher_avatar.png" class="rounded-circle" width="80" alt="User Avatar">
              <%}%>
            <h5 class="text-white mt-2 mb-0"><%= name.toUpperCase() %></h5>
            <small class="text-white"><%= role.toUpperCase() %></small>
          </div>
          
          <ul class="nav flex-column">
            <% if(role === 'student') { %>
              <!-- Student Menu -->
              <li class="nav-item">
                <a class="nav-link active" href="/dashboard">
                  <i class="bi bi-speedometer2 me-2"></i> Dashboard
                </a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/grades">
                  <i class="bi bi-journal-bookmark me-2"></i> My Grades
                </a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/attendance">
                  <i class="bi bi-calendar-check me-2"></i> Attendance
                </a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/timetable">
                  <i class="bi bi-table me-2"></i> Timetable
                </a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/assignments">
                  <i class="bi bi-list-task me-2"></i> Assignments
                </a>
              </li>
              
            <% } else if(role === 'parent') { %>
              <!-- Parent Menu -->
              <li class="nav-item">
                <a class="nav-link active" href="/dashboard">
                  <i class="bi bi-speedometer2 me-2"></i> Dashboard
                </a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/child-progress">
                  <i class="bi bi-graph-up me-2"></i> Child's Progress
                </a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/attendance">
                  <i class="bi bi-calendar-check me-2"></i> Attendance
                </a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/messages">
                  <i class="bi bi-chat-left-text me-2"></i> Teacher Messages
                </a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/school-events">
                  <i class="bi bi-calendar-event me-2"></i> School Events
                </a>
              </li>
              
            <% } else if(role === 'teacher') { %>
              <!-- Teacher Menu -->
              <li class="nav-item">
                <a class="nav-link active" href="/dashboard">
                  <i class="bi bi-speedometer2 me-2"></i> Dashboard
                </a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/class-management">
                  <i class="bi bi-people-fill me-2"></i> Class Management
                </a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/gradebook">
                  <i class="bi bi-journal-check me-2"></i> Gradebook
                </a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/attendance">
                  <i class="bi bi-calendar-check me-2"></i> Attendance
                </a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/messages">
                  <i class="bi bi-chat-left-text me-2"></i> Parent Messages
                </a>
              </li>
            <% } %>
            
            <!-- Common Menu Items -->
            <li class="nav-item">
              <a class="nav-link" href="/settings">
                <i class="bi bi-gear me-2"></i> Settings
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/logout">
                <i class="bi bi-box-arrow-right me-2"></i> Logout
              </a>
            </li>
          </ul>
        </div>
      </div>
    </div>

    <!-- Regular Sidebar (visible on larger screens) -->
    <nav id="desktop-sidebar" class="col-md-3 col-lg-2 d-none d-lg-block bg-dark sidebar">
      <div class="position-sticky pt-3">
        <div class="text-center mb-4">
          <% if(role === 'student') { %>
             <img src="/images/student_avatar.png" class="rounded-circle" width="80" alt="User Avatar">
            <%}%>
         
            <% if(role === 'parent') { %>
             <img src="/images/parent_avatar.png" class="rounded-circle" width="80" alt="User Avatar">
            <%}%>

            <% if(role === 'teacher') { %>
             <img src="/images/teacher_avatar.png" class="rounded-circle" width="80" alt="User Avatar">
            <%}%>
          <h5 class="text-white mt-2 mb-0"><%= name.toUpperCase() %></h5>
          <small class="text-white"><%= role.toUpperCase() %></small>
        </div>
        
        <ul class="nav flex-column">
          <% if(role === 'student') { %>
            <!-- Student Menu -->
            <li class="nav-item">
              <a class="nav-link active" href="/dashboard">
                <i class="bi bi-speedometer2 me-2"></i> Dashboard
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/grades">
                <i class="bi bi-journal-bookmark me-2"></i> My Grades
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/attendance">
                <i class="bi bi-calendar-check me-2"></i> Attendance
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/timetable">
                <i class="bi bi-table me-2"></i> Timetable
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/assignments">
                <i class="bi bi-list-task me-2"></i> Assignments
              </a>
            </li>
            
          <% } else if(role === 'parent') { %>
            <!-- Parent Menu -->
            <li class="nav-item">
              <a class="nav-link active" href="/dashboard">
                <i class="bi bi-speedometer2 me-2"></i> Dashboard
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/child-progress">
                <i class="bi bi-graph-up me-2"></i> Child's Progress
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/attendance">
                <i class="bi bi-calendar-check me-2"></i> Attendance
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/messages">
                <i class="bi bi-chat-left-text me-2"></i> Teacher Messages
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/school-events">
                <i class="bi bi-calendar-event me-2"></i> School Events
              </a>
            </li>
            
          <% } else if(role === 'teacher') { %>
            <!-- Teacher Menu -->
            <li class="nav-item">
              <a class="nav-link active" href="/dashboard">
                <i class="bi bi-speedometer2 me-2"></i> Dashboard
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/class-management">
                <i class="bi bi-people-fill me-2"></i> Class Management
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/gradebook">
                <i class="bi bi-journal-check me-2"></i> Gradebook
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/attendance">
                <i class="bi bi-calendar-check me-2"></i> Attendance
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/messages">
                <i class="bi bi-chat-left-text me-2"></i> Parent Messages
              </a>
            </li>
          <% } %>
          
          <!-- Common Menu Items -->
          <li class="nav-item">
            <a class="nav-link" href="/settings">
              <i class="bi bi-gear me-2"></i> Settings
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/logout">
              <i class="bi bi-box-arrow-right me-2"></i> Logout
            </a>
          </li>
        </ul>
      </div>
    </nav>

    <!-- Main Content -->
    <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4 py-4">
      <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
        <h1 class="h2">Dashboard</h1>
        <div class="btn-toolbar mb-2 mb-md-0">
          <div class="btn-group me-2">
            <button type="button" class="btn btn-sm btn-outline-secondary">
              <i class="bi bi-calendar me-1"></i> <%= new Date().toLocaleDateString() %>
            </button>
          </div>
        </div>
      </div>

      <!-- Dashboard Content Based on Role -->
      <% if(role === 'student') { %>
        <!-- Student Dashboard -->
        <div class="row">
          <!-- Grades Summary -->
          <div class="col-md-6 mb-4">
            <div class="card h-100">
              <div class="card-header bg-primary text-white">
                <h5 class="card-title mb-0"><i class="bi bi-journal-bookmark me-2"></i> Academic Summary</h5>
              </div>
              <div class="card-body">
                <div class="text-center mb-3">
                  <div class="progress-circle" data-value="85">
                    <span class="progress-circle-value">85%</span>
                  </div>
                  <h5 class="mt-3">Overall Average</h5>
                </div>
                <div class="table-responsive">
                  <table class="table table-sm">
                    <thead>
                      <tr>
                        <th>Subject</th>
                        <th>Grade</th>
                        <th>Progress</th>
                      </tr>
                    </thead>
                    <tbody>
                      <tr>
                        <td>Mathematics</td>
                        <td>A</td>
                        <td>
                          <div class="progress" style="height: 10px;">
                            <div class="progress-bar bg-success" style="width: 90%"></div>
                          </div>
                        </td>
                      </tr>
                      <tr>
                        <td>Science</td>
                        <td>B+</td>
                        <td>
                          <div class="progress" style="height: 10px;">
                            <div class="progress-bar bg-info" style="width: 85%"></div>
                          </div>
                        </td>
                      </tr>
                      <tr>
                        <td>English</td>
                        <td>A-</td>
                        <td>
                          <div class="progress" style="height: 10px;">
                            <div class="progress-bar bg-success" style="width: 88%"></div>
                          </div>
                        </td>
                      </tr>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>

          <!-- Upcoming Assignments -->
          <div class="col-md-6 mb-4">
            <div class="card h-100">
              <div class="card-header bg-warning text-dark">
                <h5 class="card-title mb-0"><i class="bi bi-list-task me-2"></i> Upcoming Assignments</h5>
              </div>
              <div class="card-body">
                <div class="list-group list-group-flush">
                  <div class="list-group-item">
                    <div class="d-flex w-100 justify-content-between">
                      <h6 class="mb-1">Math Homework</h6>
                      <small class="text-danger">Due tomorrow</small>
                    </div>
                    <p class="mb-1">Chapter 5 Exercises</p>
                    <small>Mathematics - Mr. Johnson</small>
                  </div>
                  <div class="list-group-item">
                    <div class="d-flex w-100 justify-content-between">
                      <h6 class="mb-1">Science Project</h6>
                      <small class="text-warning">Due in 3 days</small>
                    </div>
                    <p class="mb-1">Solar System Model</p>
                    <small>Science - Ms. Parker</small>
                  </div>
                  <div class="list-group-item">
                    <div class="d-flex w-100 justify-content-between">
                      <h6 class="mb-1">English Essay</h6>
                      <small class="text-primary">Due next week</small>
                    </div>
                    <p class="mb-1">Book Analysis: To Kill a Mockingbird</p>
                    <small>English - Mrs. Smith</small>
                  </div>
                </div>
              </div>
            </div>
          </div>

          <!-- Attendance Summary -->
          <div class="col-md-6 mb-4">
            <div class="card h-100">
              <div class="card-header bg-info text-white">
                <h5 class="card-title mb-0"><i class="bi bi-calendar-check me-2"></i> Attendance Summary</h5>
              </div>
              <div class="card-body">
                <div class="row text-center">
                  <div class="col-4">
                    <h3 class="text-success">95%</h3>
                    <small>Present</small>
                  </div>
                  <div class="col-4">
                    <h3 class="text-warning">3%</h3>
                    <small>Late</small>
                  </div>
                  <div class="col-4">
                    <h3 class="text-danger">2%</h3>
                    <small>Absent</small>
                  </div>
                </div>
                <hr>
                <h6>Recent Absences:</h6>
                <ul class="list-unstyled">
                  <li><i class="bi bi-dot"></i> March 15 - Sick</li>
                  <li><i class="bi bi-dot"></i> February 28 - Family Event</li>
                </ul>
              </div>
            </div>
          </div>

          <!-- Timetable Preview -->
          <div class="col-md-6 mb-4">
            <div class="card h-100">
              <div class="card-header bg-success text-white">
                <h5 class="card-title mb-0"><i class="bi bi-table me-2"></i> Today's Classes</h5>
              </div>
              <div class="card-body">
                <div class="list-group list-group-flush">
                  <div class="list-group-item">
                    <div class="d-flex w-100 justify-content-between">
                      <h6 class="mb-1">Mathematics</h6>
                      <small>9:00 - 10:00</small>
                    </div>
                    <small>Room 201 - Mr. Johnson</small>
                  </div>
                  <div class="list-group-item">
                    <div class="d-flex w-100 justify-content-between">
                      <h6 class="mb-1">Science</h6>
                      <small>10:15 - 11:15</small>
                    </div>
                    <small>Lab 3 - Ms. Parker</small>
                  </div>
                  <div class="list-group-item">
                    <div class="d-flex w-100 justify-content-between">
                      <h6 class="mb-1">English</h6>
                      <small>11:30 - 12:30</small>
                    </div>
                    <small>Room 105 - Mrs. Smith</small>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>

      <% } else if(role === 'parent') { %>
        <!-- Parent Dashboard -->
        <div class="row">
          <!-- Child Summary -->
          <div class="col-md-6 mb-4">
            <div class="card h-100">
              <div class="card-header bg-primary text-white">
                <h5 class="card-title mb-0"><i class="bi bi-person-video2 me-2"></i> Child Summary</h5>
              </div>
              <div class="card-body">
                <div class="text-center mb-3">
                  <img src="/images/student-avatar.jpg" class="rounded-circle mb-2" width="80" alt="Child Avatar">
                  <h5>Emily Rodriguez</h5>
                  <p class="text-muted">Grade 10 - Class B</p>
                </div>
                <div class="row text-center">
                  <div class="col-4">
                    <h4 class="text-primary">85%</h4>
                    <small>Average Grade</small>
                  </div>
                  <div class="col-4">
                    <h4 class="text-success">94%</h4>
                    <small>Attendance</small>
                  </div>
                  <div class="col-4">
                    <h4 class="text-warning">3</h4>
                    <small>Pending Assignments</small>
                  </div>
                </div>
              </div>
            </div>
          </div>

          <!-- Recent Grades -->
          <div class="col-md-6 mb-4">
            <div class="card h-100">
              <div class="card-header bg-info text-white">
                <h5 class="card-title mb-0"><i class="bi bi-journal-bookmark me-2"></i> Recent Grades</h5>
              </div>
              <div class="card-body">
                <div class="table-responsive">
                  <table class="table table-sm">
                    <thead>
                      <tr>
                        <th>Subject</th>
                        <th>Assignment</th>
                        <th>Grade</th>
                      </tr>
                    </thead>
                    <tbody>
                      <tr>
                        <td>Mathematics</td>
                        <td>Algebra Test</td>
                        <td class="text-success">A</td>
                      </tr>
                      <tr>
                        <td>Science</td>
                        <td>Chemistry Quiz</td>
                        <td class="text-info">B+</td>
                      </tr>
                      <tr>
                        <td>English</td>
                        <td>Essay Draft</td>
                        <td class="text-success">A-</td>
                      </tr>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>

          <!-- Teacher Messages -->
          <div class="col-md-12 mb-4">
            <div class="card h-100">
              <div class="card-header bg-warning text-dark">
                <h5 class="card-title mb-0"><i class="bi bi-chat-left-text me-2"></i> Teacher Messages</h5>
              </div>
              <div class="card-body">
                <div class="list-group list-group-flush">
                  <div class="list-group-item">
                    <div class="d-flex w-100 justify-content-between">
                      <h6 class="mb-1">Mr. Johnson - Mathematics</h6>
                      <small>2 days ago</small>
                    </div>
                    <p class="mb-1">Emily is doing well in algebra but could benefit from extra practice with quadratic equations.</p>
                    <button class="btn btn-sm btn-outline-primary mt-2">Reply</button>
                  </div>
                  <div class="list-group-item">
                    <div class="d-flex w-100 justify-content-between">
                      <h6 class="mb-1">Ms. Parker - Science</h6>
                      <small>1 week ago</small>
                    </div>
                    <p class="mb-1">Reminder: Science project due next Tuesday. Emily has selected a great topic!</p>
                    <button class="btn btn-sm btn-outline-primary mt-2">Reply</button>
                  </div>
                </div>
                <div class="mt-3">
                  <button class="btn btn-primary w-100">New Message</button>
                </div>
              </div>
            </div>
          </div>
        </div>

      <% } else if(role === 'teacher') { %>
        <!-- Teacher Dashboard -->
        <div class="row">
          <!-- Class Summary -->
          <div class="col-md-6 mb-4">
            <div class="card h-100">
              <div class="card-header bg-primary text-white">
                <h5 class="card-title mb-0"><i class="bi bi-people-fill me-2"></i> Class Summary</h5>
              </div>
              <div class="card-body">
                <h5>Grade 10 Mathematics - Section A</h5>
                <p class="text-muted">25 students | Mon/Wed/Fri 9:00-10:00</p>
                <hr>
                <div class="row text-center">
                  <div class="col-4">
                    <h4 class="text-primary">84%</h4>
                    <small>Class Average</small>
                  </div>
                  <div class="col-4">
                    <h4 class="text-success">92%</h4>
                    <small>Attendance</small>
                  </div>
                  <div class="col-4">
                    <h4 class="text-warning">5</h4>
                    <small>Late Assignments</small>
                  </div>
                </div>
              </div>
            </div>
          </div>

          <!-- Upcoming Lessons -->
          <div class="col-md-6 mb-4">
            <div class="card h-100">
              <div class="card-header bg-info text-white">
                <h5 class="card-title mb-0"><i class="bi bi-journal-bookmark me-2"></i> Upcoming Lessons</h5>
              </div>
              <div class="card-body">
                <div class="list-group list-group-flush">
                  <div class="list-group-item">
                    <div class="d-flex w-100 justify-content-between">
                      <h6 class="mb-1">Quadratic Equations</h6>
                      <small>Today, 9:00 AM</small>
                    </div>
                    <p class="mb-1">Room 201</p>
                    <small>Materials: Textbook Chapter 5, Worksheet 3</small>
                  </div>
                  <div class="list-group-item">
                    <div class="d-flex w-100 justify-content-between">
                      <h6 class="mb-1">Polynomial Functions</h6>
                      <small>Wednesday, 9:00 AM</small>
                    </div>
                    <p class="mb-1">Room 201</p>
                    <small>Materials: Graphing calculators needed</small>
                  </div>
                </div>
              </div>
            </div>
          </div>

          <!-- Gradebook Alerts -->
          <div class="col-md-6 mb-4">
            <div class="card h-100">
              <div class="card-header bg-warning text-dark">
